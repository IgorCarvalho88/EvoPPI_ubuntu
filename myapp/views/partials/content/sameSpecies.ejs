         <!--Content for Species-->
        <div class = "row">
            <div class="col-lg-6 species1">
                <div class="form-group">
                    <label class="control-label" for="company">Species1</label>
                        <div class="">
                            <select id="mySelect" class="form-control" onchange = "mySelect()">
                            <option disabled selected value> -- select an option -- </option>
                                <% species.forEach(function(species2){%>
                                    <option><%= species2 %></option>
                                <% }); %>
                            </select> 
                        </div>
                </div>
            </div>

            <div class="col-lg-6 species1">
                <div class="form-group">
                    <label class="control-label" for="company">Species2</label>
                        <div class="">
                            <select id="mySelect2" class="form-control" onchange = "mySelect2()">
                            <option disabled selected value> -- select an option -- </option>
                                <% species.forEach(function(species2){%>
                                    <option><%= species2 %></option>
                                <% }); %>
                            </select> 
                        </div>
                </div>
            </div>
        </div>


    <form id = "sameSpecies">

        <!--Content for interactome-->
        <div class = "row">
            <div class="col-lg-6 species1">
                <div class="form-group">
                    <label class="control-label" for="company">Interactome1</label>
                        <div class="">
                            <select name = interactome1 id="interactome1" class="form-control">
                            </select> 
                        </div>
                </div>
            </div>

            <div class="col-lg-6 species1">
                <div class="form-group">
                    <label class="control-label" for="company">Interactome2</label>
                        <div class="">
                            <select name = interactome2 id="interactome2" class="form-control">
                            </select> 
                        </div>
                </div>
            </div>
        </div>

        <!--Content for gene-->
        <div class = "row">
            <div class="col-lg-6 col-lg-offset-3 species1">
                    <fieldset>
                        <div class="form-group">
                            <label for="query">Genes</label>
                            <input class="form-control" name="gene" id="query" placeholder="Start typing something to search genes" type="text">              
                        </div>
                    </fieldset>
             </div>
        </div>
        <button type="submit" class="btn btn-primary">Compare</button>
    </form>

     
